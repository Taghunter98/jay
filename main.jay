func main() {
    let a: int = 5;
    let b: int = 10;
    
    let res: int = add(a, b);
    let res2: int = mult(a, b);

    let name: string = "Josh";

    print(res);
    print(res2);

    hello(name);
}

func add(a: int, b: int) -> int {
    return a + b;
}

func mult(a: int, b: int) -> int {
    return a * b;
}

func hello(name: string) {
    print(name);
}